        "PlatformAccess/AsyncLazy.cs",
        "PlatformAccess/DotNetDeltas.cs",
        "PlatformAccess/DotNetCorePlatformAccess.cs",
        "PlatformAccess/DotNetPlatformBase.cs",
        "PlatformAccess/IdManager.cs",
        "PlatformAccess/PlatformAccess.cs",
        "PlatformAccess/TaskShim.cs",
        "Properties/AssemblyInfo.cs",

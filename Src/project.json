        "Properties/AssemblyInfoNetCore.cs",
        "ServiceExceptionEventArgs.cs",
        "BuddyServiceClient/IRemoteMethodProvider.cs",
        "IRestProvider.cs",
        "Models/Blob.cs",
        "Models/Checkin.cs",
        "Models/Message.cs",
        "Models/MetadataItem.cs",
        "Models/Notification.cs",
        "Models/Picture.cs",
        "Models/User.cs",
        "Models/UserList.cs",
        "BuddyGeoLocation.cs",

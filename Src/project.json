{
  "name": "BuddyPlatformSdkNetCore",
  "version": "0.0.1-pre1",
  "packOptions": {
    "projectUrl": "http://github.com/BuddyPlatform/Buddy-DotNet-SDK",
    "licenseUrl": "https://raw.githubusercontent.com/BuddyPlatform/Buddy-DotNET-SDK/master/LICENSE.md",
    "repository": {
      "type": "git",
      "url": "git://github.com/BuddyPlatform/Buddy-DotNet-SDK"
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.1",
    "System.Net.Requests": "4.0.11",
    "System.Threading.Thread": "4.0.0",
    "System.AppDomain": "2.0.11"
